export VUG_LOCAL_CARLA_PATH=$HOME/CARLA_0.9.10

if [[ ! -z $VUG_VPN_LOCAL_ADDRESS ]]; then
    export VUG_LOCAL_ADDRESS=$VUG_VPN_LOCAL_ADDRESS
fi